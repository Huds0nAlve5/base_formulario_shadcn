generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}


model Visao {
  id        String   @id @default(dbgenerated("uuidv7()")) @db.Uuid
  nome      String
  link      String
  cpf       String   @unique               
  updatedAt DateTime @updatedAt        
}

model Usuario {
  id        String   @id @default(dbgenerated("uuidv7()")) @db.Uuid
  nome      String
  sobrenome      String
  senha       String             
  updatedAt DateTime @updatedAt      
  //usuarioId String

  //usuario   Usuario  @relation(fields: [usuarioId], references: [id])
}